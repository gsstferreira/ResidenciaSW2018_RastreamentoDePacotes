@using AplicacaoArduino.ViewModels
@model ListContainerLocalizacao

<div>
    <br />
    <br />
    <br />
    <div class="row">
        <div class="col-md-12">
            <table class="table-bordered table">
                <thead>
                    <tr>
                        <th>
                            ID rota
                        </th>
                        <th>
                            Data Início
                        </th>
                        <th>
                            Latitude
                        </th>
                        <th>
                            Longitude
                        </th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model.localizacoes)
                    {
                        <tr>
                            <td>
                                @item.LocalizacaoId
                            </td>
                            <td>
                                @item.HorarioAmostra
                            </td>
                            <td>
                                @item.Latitude
                            </td>
                            <td>
                                @item.Longitude
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
